{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/school-notice-app/dist/index.html"
    }
  ],
  "buildCommand": "cd school-notice-app && npm install && npm run build",
  "outputDirectory": "school-notice-app/dist",
  "installCommand": "npm install",
  "framework": null
}