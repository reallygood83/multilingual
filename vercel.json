{
  "version": 2,
  "builds": [
    {
      "src": "school-notice-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/school-notice-app/dist/$1"
    }
  ]
}